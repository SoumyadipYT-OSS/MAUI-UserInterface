<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="T01_contentType.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:T01_contentType"
    xmlns:ImportPagesFolder="clr-namespace:T01_contentType.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="T01_contentType">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="Example1"
        ContentTemplate="{DataTemplate ImportPagesFolder:example1}"
        Route="example1" />

    <ShellContent
        Title="Example2"
        ContentTemplate="{DataTemplate ImportPagesFolder:example2}"
        Route="example2" />

</Shell>
