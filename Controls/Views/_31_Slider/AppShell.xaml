<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="_31_Slider.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:_31_Slider"
    xmlns:pages="clr-namespace:_31_Slider.Pages"
    Shell.FlyoutBehavior="Flyout"
    Title="_31_Slider">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="SliderTutorial"
        ContentTemplate="{DataTemplate pages:SliderTutorial}"
        Route="SliderTutorial" />

</Shell>
