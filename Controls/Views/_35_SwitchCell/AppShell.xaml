<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="_35_SwitchCell.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:_35_SwitchCell"
    xmlns:csc="clr-namespace:_35_SwitchCell.CustomSwitchCell"
    Shell.FlyoutBehavior="Flyout"
    Title="_35_SwitchCell">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <ShellContent
        Title="SwitchCell Demo Page"
        ContentTemplate="{DataTemplate csc:SwitchCellDemoPage}"
        Route="SwitchCellDemoPage"
        />

</Shell>